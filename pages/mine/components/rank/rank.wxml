<!--pages/mine/rank.wxml-->
<view class="rank-all-wrap">
  
  <view class="rank_name">{{rankTitle}}</view>
  <view class="rank-wrap">
    <view class="num-wrap flex-colum-center" style="left={{percentage}}rpx">
      <view class="num">{{currNum}}</view>
      <view class="triangle-down"></view>
    </view>
    <view class="all-num">/ {{allNum}}</view>
    <view class="bar"></view>
    <view class="circle"></view>
  </view>
</view>